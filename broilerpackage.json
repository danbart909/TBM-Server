{
  "name": "broilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "start": "node src/server",
    "dev": "cross-env nodemon src/server",
    "migrate": "cross-env postgrator --config postgrator-config.js",
    "migrate:test": "cross-env env NODE_ENV=test npm run migrate",
    "migrate:production": "cross-env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "predeploy": "npm audit && npm run migrate:production",
    "deploy": "git push heroku master"
  },
  "keywords": [],
  "engines": {
    "node": "12.13.1 node --version ------------------------------------"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "knex": "^0.20.10",
    "morgan": "^1.9.1",
    "pg": "^7.18.2",
    "postgrator-cli": "^3.3.0",
    "valid-url": "^1.0.9",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
}
